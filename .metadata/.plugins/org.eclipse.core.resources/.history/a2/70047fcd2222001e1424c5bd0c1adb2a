package com.demo.service;

import com.demo.bean.LoginBean;
import com.demo.dao.LoginDao;
import com.demo.dao.LoginDaoImpl;
import com.demo.dao.RegistrationDao;
import com.demo.dao.RegistrationDaoImpl;

public class RegistrationServiceImpl implements RegistrationService {
	
	RegistrationDao registrationdao = new RegistrationDaoImpl();
	@Override
	public LoginBean insertUser(String user, String pass , String role) {
		// TODO Auto-generated method stub
		return registrationDao.insertUser(user, pass,role);
	}

}
